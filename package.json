{
  "name": "vue-webauthen-nosql",
  "version": "0.1.0",
  "description": "Fullstack WebAuthn package with Vue 3 frontend and Node.js + Mongo backend",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev:server": "nodemon --watch server --ext ts --exec ts-node server/index.ts",
    "dev:client": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.js,.vue",
    "start": "node dist/server/index.js"
  },
  "keywords": [
    "webauthn",
    "vue3",
    "nodejs",
    "mongodb",
    "fullstack",
    "authentication",
    "passkey"
  ],
  "author": "Daniyal Frd",
  "license": "MIT",
  "dependencies": {
    "@simplewebauthn/server": "^5.1.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.10.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/compiler-sfc": "^3.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^4.4.9"
  }
}
